        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle state
switch state
{
case 1: // move offsets (message)
    if title_offset1 title_offset1 -= 16;
    if title_offset2 title_offset2 -= 16;
    break;

case 2: // move offsets (scores)
    if score_offset1 score_offset1 -= 16;
    if score_offset2 score_offset2 -= 16;
    if score_offset3 score_offset3 -= 16;
    break;

case 3: // apply score
    if time_bonus
    {
        total_bonus += min(time_bonus, 100);
        objGameData.player_score[0] += min(time_bonus, 100);
        objGameData.player_score[1] += min(time_bonus, 100);
        time_bonus = max(time_bonus-100, 0);
    }
    if rings_bonus
    {
        total_bonus += min(rings_bonus, 100);
        objGameData.player_score[0] += min(rings_bonus, 100);
        objGameData.player_score[1] += min(rings_bonus, 100);
        rings_bonus = max(rings_bonus-100, 0);
    }
    if not (time_bonus or rings_bonus)
    {
        audio_stop_sound(sndBeep);
        audio_play_sound(sndChing, 0, false);
        alarm[3] = 220;
        state += 1;
    }
    else if not (objScreen.image_index mod 4)
    {
        audio_stop_sound(sndBeep);
        audio_play_sound(sndBeep, 0, false);
    }
    break;
}
</string>
