        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Start level

// use temporary data if available
if objProgram.temp_spawn_id
{
    // reset time
    timer = objProgram.temp_spawn_time;

    // add player(s)
    player[0] = instance_create(objProgram.temp_spawn_id.x, objProgram.temp_spawn_id.y+1, objPlayer);
    if objGameData.character_id[1] player[1] = instance_create(objProgram.temp_spawn_id.x, objProgram.temp_spawn_id.y+1, objPlayer);

    // reset ids
    objProgram.temp_spawn_id = noone;
    objProgram.temp_spawn_time = -1;
}
else
{
    // reset time
    timer = objProgram.spawn_time;

    // add player(s)
    player[0] = instance_create(objProgram.spawn_id.x, objProgram.spawn_id.y+1, objPlayer);
    if objGameData.character_id[1] player[1] = instance_create(objProgram.spawn_id.x, objProgram.spawn_id.y+1, objPlayer);
}

// create particle system
particles = part_system_create();

// play music
audio_set_priority(music, 1);
audio_play_priority(true);

// create hud
instance_create(0, 0, objHud);
</string>
