        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Toggle pausing
if started and not instance_exists(objTransition) and input_check_pressed(0, cSTART)
{
    // switch pause flag
    paused = not paused;

    // toggle timer
    timer_enabled = not paused;

    // pause screen frames
    objScreen.image_speed = not paused;
    objScreen.paused = paused;

    // pause particle system
    part_system_automatic_update(particles, not paused);

    // toggle all active instances
    if paused
    {
        create_screenshot();
        instance_deactivate_object(objSceneObject);
        audio_pause_all();
        audio_pause_music();
    }
    else
    {
        instance_activate_object(objSceneObject);
        audio_resume_all();
        audio_resume_music();
    }
}
</string>
